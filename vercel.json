{
  "name": "meteo",
  "builds": [{ "src": "**/*.js", "use": "@vercel/node" }],
  "routes": [
    {"src": "/data", "methods": ["GET", "OPTIONS"], "dest": "data/getDatasets.js" },
    { "src": "/data", "methods": ["POST", "OPTIONS"], "dest": "data/postDataset.js" },
    { "src": "/", "dest": "/index.js" }
  ],
  "env": {
    "DB_URI": "@db-uri",
    "MY_KEY": "@my-key"
  }
}